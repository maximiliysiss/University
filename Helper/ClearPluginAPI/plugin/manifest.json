{
  "manifest_version": 2,

  "name": "Clear history",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/sync.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "cookies",
    "browsingData",
	"history",
    "http://localhost:5000/*"
  ],
  "background" : {
    "scripts" : ["js/background.js"]
  } 
}