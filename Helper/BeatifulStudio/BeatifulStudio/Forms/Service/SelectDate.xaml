<Window x:Class="BeatifulStudio.Forms.Service.SelectDate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeatifulStudio.Forms.Service"
        mc:Ignorable="d"
        Title="Select Date" Width="200" Height="220">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="20"></RowDefinition>
        </Grid.RowDefinitions>
        <Calendar Name="Calendar"></Calendar>
        <Button Click="Button_Click" Grid.Row="1" VerticalAlignment="Center">Select date</Button>
    </Grid>
</Window>
