<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.school.library">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.application.library.app.App"
        android:usesCleartextTraffic="true">
        <activity android:name="com.application.library.ui.BookReadActivity"></activity>
        <activity android:name="com.application.library.ui.BookActivity" />
        <activity android:name="com.application.library.ui.RegisterActivity" />
        <activity android:name="com.application.library.ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.application.library.ui.MainActivity"></activity>
    </application>

</manifest>